{

  "description": "Forces your browser to use old twitter.",
  "manifest_version": 2,
  "name": "GoodTwitter",
  "version": "1.8",

  "permissions": [
    "webRequest",
    "webRequestBlocking",
    "https://*.twitter.com/",
    "tabs",
    "browsingData"
  ],

  "background": {
    "scripts": ["background.js"]
  },
  
  "content_scripts": [
    {
      "matches": ["https://*.twitter.com/*"],
      "js": ["fix-paste.js"]
    }
  ],
  "icons": {
    "128": "128.png"
  }
  
}
