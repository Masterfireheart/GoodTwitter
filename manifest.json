{

  "description": "Forces your browser to use old twitter.",
  "manifest_version": 2,
  "name": "GoodTwitter",
  "version": "1.3",

  "permissions": [
    "webRequest",
    "webRequestBlocking",
    "https://*.twitter.com/"
  ],

  "background": {
    "scripts": ["background.js"]
  },
  
  "content_scripts": [
    {
      "matches": ["https://*.twitter.com/*"],
      "js": ["fix-paste.js"]
    }
  ]
  
}
